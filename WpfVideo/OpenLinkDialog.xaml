<Window x:Class="NarutoPlayer.OpenLinkDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NarutoPlayer"
        mc:Ignorable="d"
        Title="OpenLinkDialog" Height="225" Width="400">
    <Grid>
        <StackPanel Margin="70,98,70,6">
            <TextBox x:Name="linkString" Height="24"></TextBox>
            <Button Margin="25" Content="Open" Height="24" Width="92"></Button>
        </StackPanel>
    </Grid>
</Window>
